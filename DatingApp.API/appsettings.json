{
	"title": "Dating App",
	"description": "Sample .Net core 3.1 and Angular website",
	"company": "asm",
	"email": "asm2025@outlook.com",
	"website": "https://asm.com",
	"Logging": {
		"Enabled": true,
		"LogLevel": {
			"Default": "Warning",
			"Microsoft": "Warning",
			"System": "Warning",
			"Microsoft.Hosting.Lifetime": "Information",
			"Microsoft.EntityFrameworkCore": "Debug"
		}
	},
	"AllowedHosts": "*",
	"basePath": "/",
	"images": {
		"weather": {
			"url": "/files/images/weather",
			"extension": ".png"
		},
		"users": {
			"url": "/files/images/users",
			"extension": ".png",
			"size": 128 
		} 
	},
	"jwt": {
		"signingKey": "4nsvP%&3Sp5`?_qAdB=Z*8a`",
		"encryptionKey": "6fw$T_#91q2`?=v*mU^`88Re",
		"issuer": "localhost", 
		"audience": "localhost", 
		"timeout": 20
	},
	"swagger": {
		"version": "v1",
		"Description": "Dating App",
		"template": "/{documentName}/swagger",
		"ui": "/v1/swagger"
	},
	"ConnectionStrings": {
		"DefaultConnection": "Data Source=DatingApp.db"
	},
	"serilog": {
		"Using": [
			"Serilog.Sinks.Console",
			"Serilog.Sinks.File"
		],
		"Properties": {
			"Application": "Dating App"
		},
		"MinimumLevel": "Warning",
		"Override": {
			"Microsoft": "Warning",
			"System": "Warning",
			"Microsoft.Hosting.Lifetime": "Information",
			"Microsoft.EntityFrameworkCore": "Debug"
		},
		"WriteTo": [
			{
				"Name": "Console"
			},
			{
				"Name": "File",
				"Args": {
					"path": "_logs\\datingApp_.log",
					"fileSizeLimitBytes": 2147483647,
					"rollingInterval": "Day",
					"rollOnFileSizeLimit": true,
					"shared": "true",
					"outputTemplate": "{Timestamp:HH:mm:ss.fff} [{Level:u3}] {Message:lj}{NewLine}{Exception}    {Properties:j}{NewLine}",
					"formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact",
					"flushToDiskInterval": "00:00:01"
				}
			}
		],
		"Enrich": [
			"FromLogContext",
			"WithMachineName",
			"WithUserName",
			"WithHttpRequestClientHostIP",
			"WithHttpRequestClientHostName",
			"WithHttpRequestRawUrl",
			"WithHttpRequestUserAgent"
		]
	}
}
